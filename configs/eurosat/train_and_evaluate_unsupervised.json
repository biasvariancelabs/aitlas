{
    "model": {
        "classname": "aitlas.models.UnsupervisedDeepMulticlassClassifier",
        "config": {
            "num_classes": 10
        }
    },
    "task": {
        "classname": "aitlas.tasks.TrainAndEvaluateTask",
        "config": {
            "epochs": 50,
            "model_directory": "./examples/experiment/eurosat",
            "save_epochs": 5,
            "id": "eurosat",
            "train_dataset_config":{
                "classname": "aitlas.datasets.EurosatDataset",
                "config": {
                    "batch_size": 64,
                    "shuffle": true,
                    "num_workers": 4,
                    "download": false,
                    "data_dir": "/media/hdd/EuroSAT",
                    "csv_file": "/media/hdd/EuroSAT/train.csv"
                }
            },
            "val_dataset_config":{
                  "classname": "aitlas.datasets.EurosatDataset",
                  "config": {
                      "batch_size": 16,
                      "shuffle": false,
                      "num_workers": 4,
                      "download": false,
                      "data_dir": "/media/hdd/EuroSAT",
                      "csv_file": "/media/hdd/EuroSAT/val.csv"
                  }
            }
        }
    }
}
